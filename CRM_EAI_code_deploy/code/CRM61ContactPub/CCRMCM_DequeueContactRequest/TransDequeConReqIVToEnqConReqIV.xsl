          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:RCRMLocalSalutation"/>
        </local:RCRMLocalSalutation>
        <local:ContactNumber>
          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:ContactNumber"/>
        </local:ContactNumber>
        <local:ComplianceJobRole>
          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:ComplianceJobRole"/>
        </local:ComplianceJobRole>
        <local:PrimaryAccountNumber>
          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:PrimaryAccountNumber"/>
        </local:PrimaryAccountNumber>
        <local:PrimaryAccountClass>
          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:PrimaryAccountClass"/>
        </local:PrimaryAccountClass>
        <local:PrimaryAccountId>
          <xsl:value-of select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:PrimaryAccountId"/>
        </local:PrimaryAccountId>
      </local:contact>
      <xsl:for-each select="/xsdLocal:PublishContactRequestReponse/xsdLocal:PublishContactRequest/xsdLocal:Contact/xsdLocal:ContactUserId">
        <local:contactUserId>
          <local:CPfGStatusReason>
            <xsl:value-of select="xsdLocal:RCRMCPfGStatusReason"/>
          </local:CPfGStatusReason>
          <local:CPfGStatus>
            <xsl:value-of select="xsdLocal:RCRMCPfGStatus"/>
          </local:CPfGStatus>
          <local:CPfGTempURL>
            <xsl:value-of select="xsdLocal:RCRMCPfGTempURL"/>
          </local:CPfGTempURL>
          <local:CPFGUserId>
            <xsl:value-of select="xsdLocal:RCRMCPfGUserId"/>
          </local:CPFGUserId>
          <local:Id>
            <xsl:value-of select="xsdLocal:Id"/>
          </local:Id>
          <local:ContactUUID>
            <xsl:value-of select="xsdLocal:ContactUUID"/>
          </local:ContactUUID>
          <local:FTLCompleteFlag>
            <xsl:value-of select="xsdLocal:RCRMFTLCompleteFlag"/>
          </local:FTLCompleteFlag>
          <xsl:choose>
            <xsl:when test='(xsdLocal:RCRMFTLFlagSetDate != "") and (xsdLocal:RCRMFTLFlagSetDate != "00/00/0000 00:00:00")'>
              <local:FTLFlagSetDate>
                <xsl:value-of select='concat(substring(xsdLocal:RCRMFTLFlagSetDate,7.0,4.0),"-",substring(xsdLocal:RCRMFTLFlagSetDate,1.0,2.0),"-",substring(xsdLocal:RCRMFTLFlagSetDate,4.0,2.0),"T00:00:00")'/>
              </local:FTLFlagSetDate>
            </xsl:when>
            <xsl:otherwise>
              <local:FTLFlagSetDate>
                <xsl:value-of select="xsdLocal:RCRMFTLFlagSetDate"/>
              </local:FTLFlagSetDate>
            </xsl:otherwise>
          </xsl:choose>
          <local:LocalDACSId>
            <xsl:value-of select="xsdLocal:RCRMLocalDACSId"/>
          </local:LocalDACSId>
        </local:contactUserId>
      </xsl:for-each>
    </local:contactEvent>
  </xsl:template>
</xsl:stylesheet>
