<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
     xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
     version="2.5">
    <servlet>
        <description>Web Service UserRequest</description>
        <display-name>Web Service UserRequest</display-name>
        <servlet-name>UserRequest</servlet-name>
        <servlet-class>com.reuters.eai.cc.om.TransformUserRequest</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>UserRequest</servlet-name>
        <url-pattern>UserRequest</url-pattern>
    </servlet-mapping>
</web-app>
