               <comment>Siebel Rowid</comment>
            </gcap-sa:Example>
         </xsd:appinfo>
      </xsd:annotation>
      <xsd:restriction base="xsd:string">
         <xsd:pattern value="[A-Z,0-9,\-,\+]*"/>
